{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "code-gen",
	"version": "0.1.0",
	"identifier": "com.chencode-gen.app",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:3456",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "CodeGen",
				"label": "main",
				"width": 1860,
				"minWidth": 960,
				"height": 1280,
				"minHeight": 480,
				"dragDropEnabled": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
